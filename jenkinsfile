pipeline {
    agent any
    tools {nodejs "miNodejs"}
    parameters {
        string(name: 'persona_a_saludar', description: 'persona que quiero saludar')
    }
    stages {
        stage('Execution') {
            steps {
                sh 'node index.js "${persona_a_saludar}"'
            }
        }
       
    }
}
